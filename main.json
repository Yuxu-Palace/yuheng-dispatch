{
  "id": 7695905,
  "name": "main",
  "target": "branch",
  "source_type": "Repository",
  "source": "Yuxu-Palace/yuheng-dispatch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [],
      "include": ["refs/heads/main", "refs/heads/beta", "refs/heads/alpha"]
    }
  },
  "rules": [
    {
      "type": "deletion"
    },
    {
      "type": "creation"
    },
    {
