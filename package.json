{
  "name": "@yuxu-palace/yuheng-dispatch",
  "version": "0.1.0-alpha.0",
  "description": "A GitHub Action for automatic package version patching with semantic versioning workflow",
  "keywords": [
    "github-action",
    "semantic-versioning",
    "version-management",
    "npm-publishing",
    "changelog-generation"
  ],
  "author": "Yuxu-Palace",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yuxu-Palace/yuheng-dispatch.git"
  },
  "homepage": "https://github.com/Yuxu-Palace/yuheng-dispatch#readme",
  "bugs": {
    "url": "https://github.com/Yuxu-Palace/yuheng-dispatch/issues"
  },
  "main": "./dist/index.cjs",
  "files": [
    "dist",
    "action.yaml",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "check": "biome check --write",
    "format": "biome format --write"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/node": "^22.17.0",
    "@types/semver": "^7.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "pkg-types": "^2.2.0",
    "semver": "^7.7.2"
  }
}
